package sec01.exam01;

public class VarTypeExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		byte b = 127;
//		b = 128;
		
		char c = 97;
		System.out.println(c);
		c = 97 + 5;
		System.out.println(c);
		char c1 = 'a'; // 문자 하나만 저장하는 용도, 홀따옴표로 감싼다
		
		long balance = 14000;
		System.out.println(balance);

	}

}
